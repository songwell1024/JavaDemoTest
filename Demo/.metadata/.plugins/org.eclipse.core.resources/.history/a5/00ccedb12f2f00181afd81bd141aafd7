package TestTest;
import java.util.*;

public class Test2 {
	public static void main(String[] args) {
		
	}
	
	public boolean searchMatrix(int[][] matrix, int target) {
		int r = matrix.length;
		int l = matrix[0].length;
		while( (r + l)/2 != 0) {
			if(target >matrix[r/2][l/2]) {
				r = r/2;
			}
			else if(target >matrix[r/2][l/2]) {
				r = r/2;
			}
			else if(target == matrix[r/2][l/2] ) {
				return true;
			}
		}
	}

}
